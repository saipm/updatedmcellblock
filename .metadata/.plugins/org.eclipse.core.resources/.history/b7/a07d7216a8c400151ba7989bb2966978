        "but wait! A third!"
    ];
});

app.filter('unique', function() {
    return function(input, key) {
    	//alert("shjdshdshjsdhdhjdshjdshjjdsd");
        var unique = {};
        var uniqueList = [];
        for(var i = 0; i < input.length; i++){
            if(typeof unique[input[i][key]] == "undefined"){
                unique[input[i][key]] = "";
                uniqueList.push(input[i]);
            }
        }
        return uniqueList;
    };
});


app.controller('DemoController', function ($scope, $http, $q) {
    
    $scope.timeout = 5;
    $scope.delay = 3
    
    function createData () {
         var result = "";
        
        result += "json=" + encodeURI(JSON.stringify({
               text : [
                   "My timeout is ",
                   $scope.timeout,
                   " seconds and my delay is ",
                   $scope.delay,
                   " seconds."
               ].join('')
           }));
        
        result += '&delay=' + $scope.delay
        
        return result;
    }
    
    function httpRequestHandler () {
        var timeout = $q.defer(),
            result = $q.defer(),
            timedOut = false,
            httpRequest;
        
        setTimeout(function () {
            timedOut = true;
            timeout.resolve();
        }, (1000 * $scope.timeout));
        
        httpRequest = $http({
            method : 'get',
            url: 'http://localhost:8082/ngdemo/web/testcases/getalltestcases',
            data: createData(),
            cache: false,
            timeout: timeout.promise
        });
        
        httpRequest.success(function(data, status, headers, config) {
            result.resolve(data);
        });

		httpRequest.error(function(data, status, headers, config) {
            if (timedOut) {
                result.reject({
                    error: 'timeout',
                    message: 'Request took longer than ' + $scope.timeout + ' seconds.'
            });
            } else {
                result.reject(data);
            }
        });
        
        return result.promise;
    }
    
    $scope.makeRequest = function () {
        $scope.status = 'Requesting';
        $scope.response = '';
        
        var httpRequest = httpRequestHandler();
        
        httpRequest.then(function (data) {
            $scope.status = 'Complete';
            $scope.response = data.text;
        
        }, function (error) {
            $scope.status = 'Error';
            $scope.response = error;            
        });
        
    };
    
});


